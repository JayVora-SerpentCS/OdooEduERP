<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        
        <!-- Sequences for student.payslip -->
        <record id="seq_type_student_payslip" model="ir.sequence.type">
            <field name="name">Student Payslip</field>
            <field name="code">student.payslip</field>
        </record>
        
        <record id="seq_student_payslip" model="ir.sequence">
            <field name="name">Student Payslip</field>
            <field name="code">student.payslip</field>
            <field name="prefix">SLIP/</field>
            <field name="padding">3</field>
        </record>

         <!-- Sequences for student.fees.register -->
        <record id="seq_type_student_fees_register" model="ir.sequence.type">
            <field name="name">Student Fees Register</field>
            <field name="code">student.fees.register</field>
        </record>
        
        <record id="seq_student_fees_register" model="ir.sequence">
            <field name="name">Student Fees Register</field>
            <field name="code">student.fees.register</field>
            <field name="prefix">REG/</field>
            <field name="padding">3</field>
        </record>
        
        <!-- Sequences for student.fee.receipt -->
        <record id="seq_type_student_fees_receipt" model="ir.sequence.type">
            <field name="name">Student Fees Receipt</field>
            <field name="code">student.fee.receipt</field>
        </record>
        
        <record id="seq_student_fees_receipt" model="ir.sequence">
            <field name="name">Student Fees Receipt</field>
            <field name="code">student.fee.receipt</field>
            <field name="prefix">REC/</field>
            <field name="padding">3</field>
        </record>
    </data>
</openerp>
