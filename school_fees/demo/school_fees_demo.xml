            <field eval="time.strftime('%Y-%m-%d')" name="date"/>
            <field name="fees_structure_id" ref= 'demo_student_fees_structure_1'/>
            <field name="journal_id" ref="bank_journal"/>
            <field name="company_id" ref="base.main_company"/>
        </record>
